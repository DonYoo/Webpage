<form method="POST" action="/addEmployee">
	<table>
	<tr>
	 <td valign="top">
	  <label for="Fname">First Name:</label>
	 </td>
	 <td valign="top">
	  <input  type="text" name="Fname" size="30">
	 </td>
  </tr>
  <tr>
	 <td valign="top">
	  <label for="Lname">Last Name:</label>
	 </td>
	 <td valign="top">
	  <input  type="text" name="Lname" size="30">
	 </td>
  </tr>
  <tr>
 	 <td colspan="2">
  	<input type="submit" width="100"/>
   </td>
  </tr>
 </table>
</form>

